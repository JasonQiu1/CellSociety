<config>
  <Author>Prince</Author>
  <Description>Dense Mix for Clear Segregation</Description>
  <Width>10</Width>
  <Height>10</Height>
  <InitialConfig>
    <!-- First row -->
    <Cell col="0" row="0">1</Cell>
    <Cell col="1" row="0">2</Cell>
    <Cell col="2" row="0">1</Cell>
    <Cell col="3" row="0">2</Cell>
    <Cell col="4" row="0">1</Cell>
    <Cell col="5" row="0">2</Cell>
    <Cell col="6" row="0">1</Cell>
    <Cell col="7" row="0">2</Cell>
    <Cell col="8" row="0">1</Cell>
    <Cell col="9" row="0">2</Cell>
    <!-- Second row -->
    <Cell col="0" row="1">2</Cell>
    <Cell col="1" row="1">1</Cell>
    <Cell col="2" row="1">2</Cell>
    <Cell col="3" row="1">1</Cell>
    <Cell col="4" row="1">2</Cell>
    <Cell col="5" row="1">1</Cell>
    <Cell col="6" row="1">2</Cell>
    <Cell col="7" row="1">1</Cell>
    <Cell col="8" row="1">2</Cell>
    <Cell col="9" row="1">1</Cell>
    <!-- Add more rows in a similar alternating pattern, leaving a few cells empty for movement -->

    <!-- Eighth row -->
    <Cell col="0" row="7">1</Cell>
    <Cell col="1" row="7">2</Cell>
    <Cell col="2" row="7">0</Cell> <!-- Empty cell -->
    <Cell col="3" row="7">2</Cell>
    <Cell col="4" row="7">1</Cell>
    <Cell col="5" row="7">2</Cell>
    <Cell col="6" row="7">1</Cell>
    <Cell col="7" row="7">2</Cell>
    <Cell col="8" row="7">1</Cell>
    <Cell col="9" row="7">2</Cell>

    <!-- Ninth row -->
    <Cell col="0" row="8">2</Cell>
    <Cell col="1" row="8">1</Cell>
    <Cell col="2" row="8">2</Cell>
    <Cell col="3" row="8">1</Cell>
    <Cell col="4" row="8">0</Cell> <!-- Empty cell -->
    <Cell col="5" row="8">1</Cell>
    <Cell col="6" row="8">2</Cell>
    <Cell col="7" row="8">1</Cell>
    <Cell col="8" row="8">2</Cell>
    <Cell col="9" row="8">1</Cell>

    <!-- Tenth row -->
    <Cell col="0" row="9">1</Cell>
    <Cell col="1" row="9">2</Cell>
    <Cell col="2" row="9">1</Cell>
    <Cell col="3" row="9">2</Cell>
    <Cell col="4" row="9">1</Cell>
    <Cell col="5" row="9">2</Cell>
    <Cell col="6" row="9">1</Cell>
    <Cell col="7" row="9">0</Cell> <!-- Empty cell -->
    <Cell col="8" row="9">1</Cell>
    <Cell col="9" row="9">2</Cell>
  </InitialConfig>
  <Parameters>
    <segregationFactor>0.7</segregationFactor>
  </Parameters>
  <SimulationType>Segregation</SimulationType>
</config>

